		"type": "git",
		"url": "https://github.com/PowerShell/vscode-powershell.git"
	},
	"main": "./out/main",
	"activationEvents": [
		"onLanguage:powershell"
	],
	"dependencies": {
		"vscode-languageclient": "1.3.1"
	},
